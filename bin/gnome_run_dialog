#!/bin/bash

dbus-send \
    --session \
    --type=method_call \
    --dest=org.gnome.Shell \
    /org/gnome/Shell \
    org.gnome.Shell.Eval string:"if (Main.runDialog == null) { Main.runDialog = new imports.ui.runDialog.RunDialog() }"

with_xkeysnail_hack 'Main.runDialog.visible' 'Main.runDialog.open()' 'null'
